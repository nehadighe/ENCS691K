A,CAAA,CAAA,eAAA,EAAA,YAAA,CAAA,CADA;AAEA,IAAA,cAFA,4BAEA;AACA,WAAA,eAAA,GAAA,YAAA;AACA,KAJA;AAKA;AACA,IAAA,IANA,gBAMA,KANA,EAMA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,KAAA,CAAA,cAAA,GAAA,IAAA;AADA;AAAA,uBAEA,IAAA,CAAA,cAAA,CAAA,KAAA,CAAA,QAAA,EACA,IADA,CACA,YAAA;AACA;AACA;AACA,kBAAA,KAAA,CAAA,IAAA,GAAA,yBAAA,EACA,KAAA,CAAA,KAAA,GAAA,SADA,EAEA,KAAA,CAAA,KAAA,GAAA,IAFA;AAGA,kBAAA,KAAA,CAAA,cAAA,GAAA,KAAA,CANA,CAOA;AACA,iBATA,EAUA,KAVA,CAUA,UAAA,KAAA,EAAA;AACA,kBAAA,KAAA,CAAA,IAAA,GAAA,KAAA,CAAA,OAAA,EACA,KAAA,CAAA,KAAA,GAAA,SADA,EAEA,KAAA,CAAA,KAAA,GAAA,IAFA;AAGA,kBAAA,KAAA,CAAA,cAAA,GAAA,KAAA;AACA,iBAfA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KAxBA;AAyBA;AACA,IAAA,MA1BA,kBA0BA,KA1BA,EA0BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA;AACA,gBAAA,MAAA,CAAA,cAAA,GAAA,IAAA;;AAFA,sBAGA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA,eAHA;AAAA;AAAA;AAAA;;AAIA,gBAAA,MAAA,CAAA,IAAA,GAAA,wBAAA,EACA,MAAA,CAAA,KAAA,GAAA,SADA,EAEA,MAAA,CAAA,KAAA,GAAA,IAFA;AAGA,gBAAA,MAAA,CAAA,cAAA,GAAA,KAAA;AAPA;;AAAA;AAAA;AAAA,uBAUA,IAAA,CAAA,oBAAA,CACA,KAAA,CAAA,IAAA,CAAA,QADA,EAEA,KAAA,CAAA,IAFA,EAGA,KAAA,CAAA,QAHA,EAKA,IALA,CAKA,YAAA;AACA,kBAAA,KAAA,CAAA,IAAA,CAAA,aAAA,GAAA,IAAA,CADA,CACA;;AACA,kBAAA,MAAA,CAAA,SAAA,CAAA,KAAA,CAAA,IAAA,EAFA,CAEA;;;AACA,kBAAA,MAAA,CAAA,cAAA,GAAA,KAAA;;AACA,kBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,oBAAA,IAAA,EAAA;AAAA,mBAAA,EAJA,CAKA;;AACA,iBAXA,EAYA,KAZA,CAYA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,OAAA,EACA,MAAA,CAAA,KAAA,GAAA,SADA,EAEA,MAAA,CAAA,KAAA,GAAA,IAFA;AAGA,kBAAA,MAAA,CAAA,cAAA,GAAA,KAAA;AACA,iBAjBA,CAVA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4BA,KAtDA;AAuDA;AACA;AACA,IAAA,IAzDA,kBAyDA;AACA,WAAA,eAAA,GAAA,OAAA;AACA,KA3DA;AA4DA,IAAA,KA5DA,iBA4DA,KA5DA,EA4DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,cAAA,GAAA,IAAA;;AADA,oBAEA,KAAA,CAAA,KAFA;AAAA;AAAA;AAAA;;AAGA,gBAAA,MAAA,CAAA,IAAA,GAAA,iBAAA,EACA,MAAA,CAAA,KAAA,GAAA,SADA,EAEA,MAAA,CAAA,KAAA,GAAA,IAFA;AAGA,gBAAA,MAAA,CAAA,cAAA,GAAA,KAAA;AANA;;AAAA;AAUA,gBAAA,QAAA,GAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA,gBAAA,QAAA,GAAA,KAAA,CAAA,QAAA;AAXA;AAAA,uBAYA,IAAA,CAAA,MAAA,CAAA,QAAA,EAAA,QAAA,EACA,IADA,CACA,YAAA;AACA;AACA;AAEA;AACA;AACA;AACA;AACA,kBAAA,KAAA,CAAA,IAAA,CAAA,aAAA,GAAA,IAAA;;AACA,kBAAA,MAAA,CAAA,UAAA,CAAA,KAAA,CAAA,IAAA,EATA,CASA;;;AACA,kBAAA,MAAA,CAAA,cAAA,GAAA,KAAA;;AACA,kBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,oBAAA,IAAA,EAAA;AAAA,mBAAA;AACA,iBAbA,EAcA,KAdA,CAcA,UAAA,KAAA,EAAA;AACA,kBAAA,MAAA,CAAA,IAAA,GAAA,KAAA,CAAA,OAAA,EACA,MAAA,CAAA,KAAA,GAAA,SADA,EAEA,MAAA,CAAA,KAAA,GAAA,IAFA;AAGA,kBAAA,MAAA,CAAA,cAAA,GAAA,KAAA;AACA,iBAnBA,CAZA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgCA,KA5FA;AA6FA,IAAA,UA7FA,wBA6FA;AACA,WAAA,aAAA;AACA;AA/FA;AApBA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-card min-width=\"450\" class=\"card-signing elevation-2\">\n      <Login\n        v-if=\"localLoginState === 'login'\"\n        v-on:login=\"login($event)\"\n        v-on:forgotPassword=\"forgotPassword()\"\n        v-on:resetState=\"resetState()\"\n        :requestLoading=\"requestLoading\"\n        :themeColor=\"themeColor\"\n      />\n      <ForgotPass\n        v-if=\"localLoginState === 'forgotPass'\"\n        v-on:send=\"send($event)\"\n        v-on:change=\"change($event)\"\n        v-on:back=\"back()\"\n        :requestLoading=\"requestLoading\"\n        :themeColor=\"themeColor\"\n      />\n    </v-card>\n    <v-snackbar :color=\"color\" :timeout=\"snacktimeout\" v-model=\"alert\">\n      <!-- <v-snackbar :color=\"bannerColor\" timeout=\"30000\" :v-model=\"bannerAlert\"> -->\n      <div class=\"d-flex flex-row align-center justify-space-between\">\n        <p class=\"mb-0\">{{ text }}</p>\n        <v-btn color=\"white\" text @click=\"alert = false\">\n          <v-icon small>mdi-window-close</v-icon>\n        </v-btn>\n      </div>\n    </v-snackbar>\n  </div>\n</template>\n\n<script>\nimport { Auth } from \"aws-amplify\";\nimport { mapActions } from \"vuex\";\nimport Login from \"./Login\";\nimport ForgotPass from \"./ForgotPass\";\n\nexport default {\n  name: \"LoginForm\",\n  components: {\n    Login,\n    ForgotPass\n  },\n  data: () => ({\n    themeColor: \"#900028\",\n    eye: false,\n    valid: true,\n    rules: {\n      required: [v => !!v || \"Required.\"]\n    },\n    snacktimeout: 8000,\n    alert: false,\n    text: null,\n    color: null,\n    requestLoading: false,\n    localLoginState: \"login\"\n  }),\n  methods: {\n    ...mapActions([\"resetAppState\", \"userLookUp\"]),\n    forgotPassword() {\n      this.localLoginState = \"forgotPass\";\n    },\n    // sending code to change password to username\n    async send(event) {\n      this.requestLoading = true;\n      await Auth.forgotPassword(event.username)\n        .then(() => {\n          // once password has been reset\n          // go back to login again\n          (this.text = \"Your code has been sent\"),\n            (this.color = \"success\"),\n            (this.alert = true);\n          this.requestLoading = false;\n          // console.log(\"line 69, send method from forgot password\", data);\n        })\n        .catch(error => {\n          (this.text = error.message),\n            (this.color = \"#900028\"),\n            (this.alert = true);\n          this.requestLoading = false;\n        });\n    },\n    // changing old password to a new password\n    async change(event) {\n      // console.log('line 80 - change', event);\n      this.requestLoading = true;\n      if (event.password != event.confirmPassword) {\n        (this.text = \"Passwords must be same\"),\n          (this.color = \"#900028\"),\n          (this.alert = true);\n        this.requestLoading = false;\n        return;\n      }\n      await Auth.forgotPasswordSubmit(\n        event.user.username,\n        event.code,\n        event.password\n      )\n        .then(() => {\n          event.user.authenticated = true // adding authentication property\n          this.userLogIn(event.user); // sending data to the store\n          this.requestLoading = false;\n          this.$router.push({ name: \"home\" });\n          // console.log(\"line 95, change method from forgot password submit\", data);\n        })\n        .catch(err => {\n          (this.text = err.message),\n            (this.color = \"#900028\"),\n            (this.alert = true);\n          this.requestLoading = false;\n        });\n    },\n    // going back from forgot password component\n    // to login component\n    back() {\n      this.localLoginState = \"login\";\n    },\n    async login(event) {\n      this.requestLoading = true;\n      if (!event.valid) {\n        (this.text = \"Input some text\"),\n          (this.color = \"#900028\"),\n          (this.alert = true);\n        this.requestLoading = false;\n        return;\n      }\n      let username, password;\n      username = event.user.username;\n      password = event.password;\n      await Auth.signIn(username, password)\n        .then(() => {\n          // should look at the pool of users in the store, and should\n          // get that user to display in the application\n          \n          // adding authentication property: logical issue!\n          // if adding it before, the header will\n          // load first before even going to the\n          // home\n          event.user.authenticated = true\n          this.userLookUp(event.user); // looking up user information\n          this.requestLoading = false;\n          this.$router.push({ name: \"home\" });\n        })\n        .catch(error => {\n          (this.text = error.message),\n            (this.color = \"#900028\"),\n            (this.alert = true);\n          this.requestLoading = false;\n        });\n    },\n    resetState() {\n      this.resetAppState();\n    }\n  }\n};\n</script>\n\n<style>\n@import \"../../assets/colors.css\";\n\n.card-signing {\n  padding: 30px;\n}\n\n.extraTextStyle {\n  transition: 0.3s ease;\n  color: #b08c62 !important;\n}\n\n.extraTextStyle:hover {\n  transition: 0.3s ease;\n  color: #ad712a !important;\n}\n</style>"],"sourceRoot":"src/components/Auth"}]}